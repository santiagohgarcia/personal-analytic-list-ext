                <!-- LINE ITEM -->
                <Annotation Term="UI.LineItem">
                    <Collection>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="PersonalId" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="PersonalId" />
