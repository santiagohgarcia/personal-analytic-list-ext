{
  "name": "personal-analytic-list-ext",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "personal-analytic-list-ext",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^2.7.1",
    "@sap/cds": "^6",
    "dotenv": "^10.0.0",
    "express": "^4",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/ux-specification": "^1.102.5",
    "@sapui5/ts-types": "^1.92.2",
    "eslint": "^7.32.0",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "start": "cds run",
    "watch": "cds watch",
    "watch-personal-analytic-list-ext": "cds watch --open personal-analytic-list-ext/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "sqlite.db"
        }
      },
      "ECPersonalInformation": {
        "kind": "odata-v2",
        "model": "srv\\external\\ECPersonalInformation",
        "[development]": {
          "credentials": {
            "requestTimeout": 30000,
            "url": "https://sandbox.api.sap.com/successfactors/odata/v2",
            "authentication": "NoAuthentication"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "sfsfDest"
          }
        }
      }
    }
  },
  "sapux": [
    "app/personal-analytic-list-ext"
  ]
}
